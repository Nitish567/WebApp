<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<display-name>LoginServlet</display-name>
  	<description></description>
  	<servlet-class>org.nitish.IMCSWebApp.LoginServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AddCustomer</servlet-name>
  	<display-name>AddCustomer</display-name>
  	<description></description>
  	<servlet-class>org.nitish.IMCSWebApp.AddCustomer</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AddProduct</servlet-name>
  	<display-name>AddProduct</display-name>
  	<description></description>
  	<servlet-class>org.nitish.IMCSWebApp.AddProduct</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AddOrder</servlet-name>
  	<display-name>AddOrder</display-name>
  	<description></description>
  	<servlet-class>org.nitish.IMCSWebApp.AddOrder</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AddCustomer</servlet-name>
  	<url-pattern>/addCustomer</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AddProduct</servlet-name>
  	<url-pattern>/addProduct</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AddOrder</servlet-name>
  	<url-pattern>/addOrder</url-pattern>
  </servlet-mapping>
</web-app>
